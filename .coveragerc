[report]
exclude_lines =
    pragma: no cover
    os.name == .nt.
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

ignore_errors = True

[run]
omit =
    dmoj/wbox/*
    dmoj/utils/debugger/win/*
    dmoj/utils/pywinjob.py
    dmoj/utils/winperfmon.py
    dmoj/utils/winutils.py
